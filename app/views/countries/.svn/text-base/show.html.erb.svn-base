<%= render "shared/control_buttons", :locals => { :instance => @country } %>

<table class="show_table">
  <tr>
    <td class="td_label"><b><%= t("simple_form.labels.name") %>:</b></td>
    <td class="td_data"><%= @country.name %></td>
  </tr>
  <tr>
    <td class="td_label"><b><%= t("simple_form.labels.state") %> :</b></td>
    <td class="td_data"><%= @country.state.try(:name) %></td>
  </tr>
</table>
