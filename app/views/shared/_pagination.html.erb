<%= will_paginate locals[:target] %>

<div class="page_select_pagination">
    <%= form_tag locals[:model_index_path], :id => locals[:form_id], :method => "get" do %>
        <%= select_tag :per_page, options_for_select([1, 2, 3, 5, 10, 20, 50, 100, 200, 500], params[:per_page]), {:class=>"styledselect", :onchange => locals[:on_change]} %>
    <% end %>
</div>
<p class="page_label_pagination"><%= t("screens.labels.show") %> #</p>

