/*globalnav*/

#globalnav {
	width:100%;
	padding:0;
	margin:0;
	list-style:none;
	line-height:1em;
}

#globalnav LI {
	float:left;
	margin:0;
	padding:0;
}

#globalnav A {
	display:block;
	color:#eeeeee;
	text-decoration:none;
	font-weight:bold;
    font-size:14px;
	background:#999999;
	margin:0;
	padding:0.25em 1em;
	border-left:1px solid #fff;
	border-top:1px solid #fff;
	border-right:1px solid #aaa;
}

#globalnav A:hover{
    background:#f0f0f0;
    color:#92b22c;
}
#globalnav A:active,
#globalnav A.here:link,
#globalnav A.here:visited {
	background:#f0f0f0;
}

#globalnav A.here:link,
#globalnav A.here:visited {
    color:#666666;
    text-decoration:none;
    cursor:text;
}